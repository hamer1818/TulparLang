print("=== 1. DİZİ İÇİNDE DİZİ ===");
print("");

arrayJson matris = [
    [1, 2, 3],
    [4, 5, 6],
    [7, 8, 9]
];

print("Matris:", matris);
print("");

// Zincirleme erişim
int val11 = matris[0][0];  // 1
int val12 = matris[0][1];  // 2
int val22 = matris[1][1];  // 5
int val33 = matris[2][2];  // 9

print("Matris[0][0] =", val11);
print("Matris[0][1] =", val12);
print("Matris[1][1] =", val22);
print("Matris[2][2] =", val33);
print("");

