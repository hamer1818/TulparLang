// ----------------------------------------
// 4. OBJECT İÇİNDE DİZİ
// ----------------------------------------
print("=== 4. OBJECT İÇİNDE DİZİ ===");
print("");

arrayJson ekip = {
    "isim": "Dev Team",
    "uyeler": [
        {"ad": "Ahmet", "rol": "Backend"},
        {"ad": "Mehmet", "rol": "Frontend"},
        {"ad": "Ayşe", "rol": "DevOps"}
    ]
};

print("Ekip:", ekip);
print("");

// Zincirleme: object -> dizi -> object
str uye1_ad = ekip["uyeler"][0]["ad"];
str uye1_rol = ekip["uyeler"][0]["rol"];

str uye2_ad = ekip["uyeler"][1]["ad"];
str uye2_rol = ekip["uyeler"][1]["rol"];

str uye3_ad = ekip["uyeler"][2]["ad"];
str uye3_rol = ekip["uyeler"][2]["rol"];

print("Üye 1:", uye1_ad, "-", uye1_rol);
print("Üye 2:", uye2_ad, "-", uye2_rol);
print("Üye 3:", uye3_ad, "-", uye3_rol);
print("");

// ----------------------------------------
// 5. KOMPLEKS NESTED YAPISI
// ----------------------------------------
print("=== 5. KOMPLEKS NESTED YAPI ===");
print("");

arrayJson proje = {
    "isim": "OLang",
    "versiyon": "1.3.0",
    "ekipler": [
        {
            "takim": "Backend",
            "lider": {
                "ad": "Hamza",
                "yetenekler": ["C", "Python", "Go"]
            },
            "uyeler": [
                {"ad": "Ali", "level": 5},
                {"ad": "Veli", "level": 7}
            ]
        }
    ]
};

print("Proje:", proje);
print("");

// Süper zincirleme erişim!
str proje_isim = proje["isim"];
str takim_isim = proje["ekipler"][0]["takim"];
str lider_ad = proje["ekipler"][0]["lider"]["ad"];
str yetenek1 = proje["ekipler"][0]["lider"]["yetenekler"][0];
str yetenek2 = proje["ekipler"][0]["lider"]["yetenekler"][1];
str uye_ad = proje["ekipler"][0]["uyeler"][0]["ad"];
int uye_level = proje["ekipler"][0]["uyeler"][0]["level"];

print("Proje:", proje_isim);
print("Takım:", takim_isim);
print("Lider:", lider_ad);
print("  Yetenek 1:", yetenek1);
print("  Yetenek 2:", yetenek2);
print("Üye:", uye_ad, "- Level:", uye_level);
print("");

